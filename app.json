{
  "expo": {
    "name": "Leadvisor",
    "slug": "app-lvc",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "heroui-native-example",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.leadvisor.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "需要访问您的相机，以便您可以拍摄和上传照片。",
        "NSPhotoLibraryUsageDescription": "需要访问您的照片库，以便您可以选择和上传照片。",
        "NSMicrophoneUsageDescription": "需要访问您的麦克风，以支持语音输入或通话。",
        "NSLocationWhenInUseUsageDescription": "需要访问您的位置信息，以提供个性化的定位服务。",
        "NSContactsUsageDescription": "需要访问您的通讯录，以方便您邀请好友或查找联系人。",
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleLocalizations": [
          "fr",
          "zh",
          "en"
        ],
        "UIBackgroundModes": []
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.leadvisor.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-image-picker",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-localization",
      "expo-secure-store",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": false,
          "supportsPictureInPicture": true
        }
      ],
      "expo-mail-composer"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "24f38426-c6d3-4941-af97-0503f5be3493"
      }
    },
    "owner": "neptia-zeng"
  }
}
